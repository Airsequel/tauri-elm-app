{
  "build": {
    "beforeDevCommand": "make dev-elm",
    "beforeBuildCommand": "make dist/main.js",
    "devUrl": "http://localhost:53023",
    "frontendDist": "../dist"
  },
  "productName": "tauri-elm-app",
  "version": "0.1.0",
  "identifier": "com.tauri-elm-app",
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Tauri Elm App",
        "width": 600,
        "height": 200
      }
    ]
  }
}
