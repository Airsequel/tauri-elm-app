{
  "build": {
    "beforeDevCommand": "make dev-elm",
    "beforeBuildCommand": "make dist/index.html",
    "devPath": "http://localhost:8000/src/Main.elm",
    "distDir": "../dist"
  },
  "package": {
    "productName": "tauri-elm-app",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri-elm-app",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Tauri Elm App",
        "width": 600,
        "height": 200
      }
    ]
  }
}
